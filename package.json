{
  "name": "decebot",
  "version": "0.1.0-alpha",
  "main": "dist/main/electron-main.js",
  "description": "A bot for twitch.tv/itsdece",
  "author": {
    "name": "ItsDece"
  },
  "scripts": {
    "postinstall": "patch-package && electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p -r electron:serve ng:serve",
    "serve": "electron . --serve",
    "build": "npm run build:prod",
    "build:run": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build:run -- -c dev",
    "build:prod": "npm run build:run -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "ng serve -c web -o",
    "electron:serve-tsc": "webpack --config src/main/webpack.main.ts",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "electron:mac": "npm run build:prod && electron-builder build --mac",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='src/renderer/e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register src/renderer/e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0 || ^10.0.0-0",
    "@angular/cdk": "^9.2.4",
    "@angular/forms": "^9.0.0 || ^10.0.0-0",
    "@angular/material": "^9.2.4",
    "devtron": "^1.4.0",
    "electron-reload": "^1.5.0",
    "obs-websocket-js": "^4.0.1",
    "say": "^0.16.0",
    "slp-parser-js": "^4.1.0",
    "sound-play": "^1.0.5",
    "tmi.js": "^1.5.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.1.0",
    "@angular-devkit/build-angular": "^0.901.4",
    "@angular/animations": "^9.1.4",
    "@angular/cli": "^9.1.4",
    "@angular/common": "^9.1.4",
    "@angular/compiler": "^9.1.4",
    "@angular/compiler-cli": "^9.1.4",
    "@angular/core": "^9.1.4",
    "@angular/forms": "^9.1.4",
    "@angular/language-service": "^9.1.4",
    "@angular/platform-browser": "^9.1.4",
    "@angular/platform-browser-dynamic": "^9.1.4",
    "@angular/router": "^9.1.4",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/mocha": "^7.0.2",
    "@types/node": "^10.13.0",
    "@types/tmi.js": "^1.4.0",
    "@types/webpack": "^4.41.17",
    "chai": "^4.2.0",
    "codelyzer": "~5.2.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "electron": "^9.0.2",
    "electron-builder": "^22.7.0",
    "electron-prebuilt-compile": "^8.2.0",
    "eslint": "^7.2.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.0",
    "karma-electron": "~6.3.0",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "mocha": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.2.2",
    "prettier": "^2.0.5",
    "protractor": "~5.4.0",
    "rxjs": "^6.5.5",
    "spectron": "^11.0.0",
    "ts-loader": "^7.0.5",
    "ts-node": "~7.0.0",
    "tslib": "^1.11.0",
    "typescript": "~3.8.2",
    "wait-on": "^5.0.1",
    "webdriver-manager": "^12.1.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "zone.js": "^0.10.3"
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
